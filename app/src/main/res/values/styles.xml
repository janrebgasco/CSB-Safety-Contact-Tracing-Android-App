<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="toggleOn">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@drawable/toggle_color</item>
        <item name="android:background">@drawable/toogle_button</item>
    </style>
    <style name="toggleOff">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@drawable/toggle_color_off</item>
        <item name="android:background">@drawable/toogle_button_off</item>
    </style>
    <style name="progressBarBlue" parent="@style/Theme.AppCompat">
        <item name="colorAccent">@color/blue</item>
    </style>

</resources>